function is_prime(n) {
    for (let i = 2; i <= sqrt(n); i++) {
        if (n % i === 0) {
            return false;
        }
    }
    return true;
}

while (true) {
    let n = prompt("请输入一个正整数:");
    if (n == "exit") {
        break;
    }
    if (is_prime(n)) {
        console.log(n + " 是素数");
    } else {
        console.log(n + " 不是素数");
    }
}
